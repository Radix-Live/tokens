{
  "compilerOptions": {
    "target": "ES2022",
    "module": "commonjs",
    "lib": [],
    "types": [
      "node"
    ],
    "strict": true,
    "noImplicitAny": true,
    "noImplicitThis": true,
    "skipLibCheck": true,
    "noImplicitReturns": true,
    "moduleResolution": "node",
    "esModuleInterop": true
  }
}
